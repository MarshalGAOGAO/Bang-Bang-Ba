<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.registerloogin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="帮帮吧"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <style
            name="AppTheme"
            parent="AppBaseTheme">
            <item name="android:windowFullscreen">
                true
            </item>
            <item name="android:windowNoTitle">
                true
            </item>
        </style>

        <activity android:name=".LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".setPasswordActivity" />
        <activity android:name=".identityActivity" />
        <activity android:name=".orderDetail1"></activity>
    </application>

</manifest>